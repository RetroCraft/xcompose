Mk ! ( 8 ) : "❽" U277D # ⋮
Mk ! ( 9 ) : "❾" U277E # ⋮
Mk ! ( 1 0 ) : "❿" U277F # DINGBAT NEGATIVE CIRCLED NUMBER TEN
Mk ! ( 1 1 ) : "⓫" U24EB # NEGATIVE CIRCLED NUMBER ELEVEN
Mk ! ( 1 2 ) : "⓬" U24EC # ⋮
Mk ! ( 1 3 ) : "⓭" U24ED # ⋮
Mk ! ( 1 4 ) : "⓮" U24EE # ⋮
Mk ! ( 1 5 ) : "⓯" U24EF # ⋮
Mk ! ( 1 6 ) : "⓰" U24F0 # ⋮
